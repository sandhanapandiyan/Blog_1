{
    "build": {
      "nixpacksPlan": {
        "phases": {
          "setup": {
            "cmds": [
              "pip install -r requirements.txt"
            ]
          },
          "build": {
            "cmds": [
              "python manage.py collectstatic --noinput"
            ]
          }
        },
        "start": {
          "cmd": "gunicorn .wsgi:application"
        }
      }
    }
  }